<!DOCTYPE html>
<html>
<head>
    <title>Subqueries</title>
</head>
    <body>
        <h1>Join Subquery</h1>
        <p>Lists customers and their personal + order information.</p>
    <p>select * from customer join (select * from order_) b ' +
               'on b.customer_id = customer.customer_id;</P>

         <table border="1">
         <tr>
         <th>Customer ID</th>
         <th>Customer Name</th>
         <th>Customer Email</th>
         <th>Customer Phone Number</th>
         <th>Order Number</th>
         <th>Customer ID</th>
         <th>Quantity</th>
         <th>Customer Address</th>
         </tr>
         <% for(var i = 0; i < join.length; i++) { %>
         <tr>
         <td><%= join[i].customer_id %></td>
         <td><%= join[i]._name %></td>
         <td><%= join[i].email %></td>
         <td><%= join[i].phone_num %></td>
         <td><%= join[i].order_num %></td>
         <td><%= join[i].customer_id %></td>
         <td><%= join[i].quantity %></td>
         <td><%= join[i].address %></td>
         </tr>
         <% } %>
         </table>
    </body>
</html>